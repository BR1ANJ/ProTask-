      "edgeToEdgeEnabled": true,
      "package": "com.brianji.miapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ],
    "extra": {
      "eas": {
        "projectId": "b4aed4da-14c2-4641-b1c7-54623c4fe84f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
